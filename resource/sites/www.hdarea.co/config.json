{
  "name": "HDArea",
  "timezoneOffset": "+0800",
  "schema": "NexusPHP",
  "url": "https://www.hdarea.co/",
  "description": "高清世界",
  "icon": "https://www.hdarea.co/favicon.ico",
  "tags": [
    "影视",
    "综合"
  ],
  "host": "www.hdarea.co",
  "collaborator": [
    "lzl20110",
    "ian"
  ],
  "supportedFeatures": {
    "search": true,
    "imdbSearch": false,
    "userData": true,
    "sendTorrent": true
  },
  "selectors": {
    "userBaseInfo": {
      "page": "/index.php",
      "fields": {
        "id": {
          "selector": [
            "#info_block > tbody > tr > td > table > tbody > tr > td:nth-child(1) > span > span > a"
          ],
          "attribute": "href",
          "filters": [
            "query ? query.getQueryString('id'):''"
          ]
        },
        "name": {
          "selector": [
            "#info_block > tbody > tr > td > table > tbody > tr > td:nth-child(1) > span > span > a > b"
          ],
          "filters": [
            "query.text().trim()"
          ]
        }
      }
    },
    "userExtendInfo": {
      "page": "/userdetails.php?id=$user.id$",
      "fields": {
        "bonus": {
          "selector": [
            "td.rowhead.nowrap:contains('魔力值')"
          ],
          "filters": [
            "parseFloat(query.next().text().trim())"
          ]
        },
        "levelName": {
          "selector": [
            "td.rowhead.nowrap:contains('等级')"
          ],
          "filters": [
            "query.next().find('img').attr('title').trim()"
          ]
        },
        "uploaded": {
          "selector": [
            "td.rowhead.nowrap:contains('传输')"
          ],
          "filters": [
            "query.next().find('td.embedded:eq(0)').text().split(':')[1].trim()",
            "query.replace(/,/g,'').sizeToNumber()"
          ]
        },
        "downloaded": {
          "selector": [
            "td.rowhead.nowrap:contains('传输')"
          ],
          "filters": [
            "query.next().find('td.embedded:eq(1)').text().split(':')[1].trim()",
            "query.replace(/,/g,'').sizeToNumber()"
          ]
        },
        "joinTime": {
          "selector": [
            "td.rowhead.nowrap:contains('加入日期')"
          ],
          "filters": [
            "query.next().text().split('(')[0].trim()",
            "dateTime(query).isValid()?dateTime(query).valueOf():query"
          ]
        }
      }
    }
  },
  "searchEntryConfig": {
    "fieldSelector": {
      "progress": {
        "selector": [
          "table[title='downloading'] > tbody > tr > td > div",
          "table[title='seeding'] > tbody > tr > td > div",
          "table[title='Stopped'] > tbody > tr > td > div",
          "table[title='completed'] > tbody > tr > td > div"
        ],
        "filters": [
          "query.attr('style')||''",
          "query.match(/width:.?(\\d.+)%/)",
          "(query && query.length>=2)?query[1]:null"
        ]
      },
      "status": {
        "selector": [
          "table[title='downloading']",
          "table[title='seeding']",
          "table[title='Stopped']",
          "table[title='completed']"
        ],
        "switchFilters": [
          [
            "1"
          ],
          [
            "2"
          ],
          [
            "3"
          ],
          [
            "255"
          ]
        ]
      }
    }
  },
